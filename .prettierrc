{
	"tabWidth": 4,
	"singleQuote": true,
	"trailingComma": "all",
	"semi": true,
	"printWidth": 130,
	"useTabs": true,
	"jsxSingleQuote": true,
	"endOfLine": "auto",
	"bracketSameLine": false,
	"htmlWhitespaceSensitivity": "ignore",
	"overrides": [
		{
			"files": [".eslintrc"],
			"options": {
				"parser": "json"
			}
		}
	]
}
